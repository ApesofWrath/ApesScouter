<%= erb :header %>

<div class="container">
    <div class="row">
        <div class="span2">
            <h2>Competitions</h2>
        </div>
    </div>
    <div class="well">
        <% Competition.each do |competition| %>
            <li><h4><a href="/competitions/<%= competition.id %>"><%= competition.name %></a></h4></li>
        <% end %>
    </div>
</div>

<%= erb :footer %>
